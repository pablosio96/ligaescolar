extends ../layout.jade
block content
    div(class="col-md-4 col-md-offset-4")
      h1(class="azul-oscuro")=title
      form(method="POST" action="/nadadores" style="margin-top: 1.5em;" enctype="urlencoded")
        div(class="form-group")
          label Temporada
          br
          select(name="id_temporada" class="form-control")
            - each temporada in temporadas
              option= temporada.id_temporada
          br
          br
        input(type="submit" class="btn btn-primary" value="Aceptar")
