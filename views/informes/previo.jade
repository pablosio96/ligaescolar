  h2(class="col-md-4 col-md-offset-4 text-center")= prueba
  p(class=" col-md-4 col-md-offset-1 ")= title
  - each serie in series
    table(class="table table-striped text-center")
      thead
        tr
          th Serie
          th= serie
          th= nombre_prueba
        tr
          th Calle
          th Dorsal
          th Nadador
          th AÃ±o
          th Marca
      -each nadador in nadadores
         if nadador.serie == serie
          tbody
            tr
              td= nadador.calle
              td= nadador.dorsal
              td= nadador.id_nadador
              td= nadador.anho_nacimiento
              td= nadador.mejor_marca
    br    
        
