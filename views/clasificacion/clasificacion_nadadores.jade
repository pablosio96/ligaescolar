extends ../layout.jade
block content
  br
  h1(class="col-md-4 col-md-offset-4 text-center azul-oscuro")= title
  
  table(class="table table-striped text-center")
    thead
      tr
      th Posicion
      th Nombre
      th Puntuacion
    tbody
      - var count = 0
      - each nadador in nadadores
        tr
          td= ++count
          td= nadador.id_nadador
          td= nadador.puntuacion_nadador
